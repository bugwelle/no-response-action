{
  "name": "no-response",
  "version": "0.0.0",
  "private": true,
  "description": "A GitHub Action that closes Issues where the author hasn't responded to a request for more information",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx ncc build ./src/main.ts",
    "ci": "npm run format-check && npm run lint && npm test",
    "format": "prettier --write **/*.ts **/*.md **/*.yaml **/*.yml",
    "format-check": "prettier --check **/*.ts **/*.md **/*.yaml **/*.yml",
    "lint": "npx eslint src/**/*.ts",
    "start": "npx ncc run ./src/main.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bugwelle/no-response-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "lee-dohm",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^7.6.1",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vercel/ncc": "^0.38.2",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^5.0.2",
    "eslint-plugin-jest": "^28.8.3",
    "extract-pr-titles": "^1.1.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "5.5"
  }
}
